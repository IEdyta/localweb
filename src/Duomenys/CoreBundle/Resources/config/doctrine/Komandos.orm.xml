<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Duomenys\CoreBundle\Entity\Komandos" table="komandos">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pavadinimas" type="string" column="Pavadinimas" length="45" nullable="true"/>
    <field name="miestas" type="string" column="Miestas" length="45" nullable="true"/>
    <field name="logotipas" type="string" column="Logotipas" length="100" nullable="true"/>
    <field name="sukis" type="string" column="Sukis" length="100" nullable="true"/>
    <field name="treneris" type="string" column="Treneris" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
